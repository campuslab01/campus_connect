{
  "routes": [
    {
      "src": "/(.*\\.js)",
      "headers": { "Content-Type": "application/javascript" }
    },
    { "src": "/(.*\\.css)", "headers": { "Content-Type": "text/css", "Cache-Control": "public, max-age=31536000" } },
    {
      "src": "/(.*\\.(png|jpg|jpeg|gif|svg|webp))",
      "headers": { "Content-Type": "image/$2",  "Cache-Control": "public, max-age=31536000" }
    },
    { "src": "/service-worker.js", "headers": { "Content-Type": "application/javascript",  "Cache-Control": "no-cache" } }
  ],
  "clean_urls": false,
  "trailing_slash": false,
  "fallback": "index.html"
}
